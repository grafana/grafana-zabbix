    "postcss-reporter": "7.0.5",
    "postcss-scss": "4.0.4",
    "prettier": "^3.0.3",
    "prop-types": "15.7.2",
    "react-table-6": "6.11.0",
    "react-use": "17.4.0",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.63.2",
    "sass-loader": "13.3.1",
    "semver": "^7.6.3",
    "style-loader": "3.3.3",
    "swc-loader": "^0.2.3",
